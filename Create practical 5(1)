#include<iostream>
using namespace std;
  
  struct collage_info{
      string collage_name ;
      int collage_coad ;
      string dept ;
      int intake ;
  };
  
int main()
{
    collage_info c1 ;
    
     //assinging a value to structure members
    
     c1. collage_name = "Government polytechnic junagadh" ;
     c1. collage_coad = 619 ;
     c1. dept = "computer";
     c1. intake = 79 ;
     
     //display on console
     
     cout << "collage_name : " <<  c1. collage_name << endl;
     cout << "collage_coad : " <<  c1. collage_coad << endl;
     cout << "dept : " <<  c1. dept << endl;
     cout << "intake  : " <<  c1. intake << endl;
     
     
    return 0 ;
}
